<odoo>
        <record id="ir_cron_update_channel_status_publish" model="ir.cron">
            <field name="name">Update Channel Status Publish</field>
            <field name="model_id" ref="model_slide_channel"/>
            <field name="state">code</field>
            <field name="code">model._cron_check_course_availability()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>      
        <record id="ir_cron_update_slide_status_publish" model="ir.cron">
            <field name="name">Update Slide Status Publish</field>
            <field name="model_id" ref="model_slide_slide"/>
            <field name="state">code</field>
            <field name="code">model._cron_check_slide_availability()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>        
</odoo>
